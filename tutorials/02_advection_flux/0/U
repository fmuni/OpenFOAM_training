FoamFile
{
    format      ascii;
    class       volVectorField;
    object      U;
}
// * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * //

dimensions      [0 1 -1 0 0 0 0];

internalField   uniform (1 1 0);

boundaryField
{
    "x0|y0|x1|y1"
    {
        type            zeroGradient;
    }
    
    dummy
    {
        type empty;
    }

}

